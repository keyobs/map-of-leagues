        }
    },
    "dependencies": {
        "less": "^4.1.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.8.1",
        "react-router-dom": "^6.8.1"
    },
    "devDependencies": {
        "@types/node": "^18.13.0",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "@vitejs/plugin-react-swc": "^3.0.0",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-react": "^7.32.2",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.1",
        "prettier": "^2.8.4",
        "typescript": "^4.9.3",
        "vite": "^4.1.0"
